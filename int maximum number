#include<iostream>
using namespace std;
int max(int a,int b, int c){
if(a>b && a>c){
return a;
}
else
if(b>a &&b>c){
return b ;
}
else
if(c>a &&c>b){
return c ;
}
}
int main()
{
    int maximum;
    int x=2, y=5, z=7;
    maximum=max(x,y,z);
    cout << " Enter the Maximum number is:" <<maximum<< endl;
    return 0;
}
