#include<iostream>
using namespace std;
inline int Multiple(int a,int b){
return a*b;
}
int main()
{
int a,b ;

    cout << "Enter the 1st number:" << endl;
    cin >>a;
    cout << "Enter the 2nd number" << endl;
    cin >> b;
    cout << "Multiplication is :" <<Multiply (a,b)<< endl;
    return 0;
}
