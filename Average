#include<iostream>
using namespace std;
int main ()
   { 

    int a,b,c;

    float average;
    cout << "Enter the first number:";
    cin >> a;
    cout << "Enter the second number: ";
    cin >> b;
    cout << "Enter the third number: ";
    average =(a+b+c/3);
    cout << "Average of the three numbers:"<<average ;
    return 0;

    
}



    
